<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:com.sixbynine.infosessions="http://schemas.android.com/apk/res-auto">

    <item android:drawable="@drawable/info_session_focused" android:state_pressed="true"
           /> <!-- pressed -->

    <item android:drawable="@drawable/info_session_focused" android:state_focused="true"
           /> <!-- focused -->

    <item android:drawable="@drawable/info_session_focused" android:state_hovered="true"
           /> <!-- hovered -->

    <item android:drawable="@drawable/info_session_end" com.sixbynine.infosessions:state_last_of_category="true"
           /> <!-- end -->

    <item android:drawable="@drawable/info_session_normal" /> <!-- default -->



</selector>